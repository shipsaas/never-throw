{
  "name": "shipsaas/never-throw",
  "type": "library",
  "version": "1.0.0",
  "description": "Response-first over Throw. PHP NeverThrow library sample implementation",
  "keywords": [
    "php",
    "library",
    "response-first",
    "neverthrow"
  ],
  "authors": [
    {
      "name": "Phat Tran (Seth Phat)",
      "email": "me@sethphat.com",
      "homepage": "https://github.com/sethsandaru",
      "role": "Sr.SWE"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1|^8.2"
  },
  "autoload": {
    "psr-4": {
      "NeverThrow\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NeverThrow\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10"
  },
  "scripts": {
    "test-coverage": [
      "@php vendor/bin/phpunit --coverage-clover coverage.xml"
    ],
    "test": [
      "@php vendor/bin/phpunit"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
